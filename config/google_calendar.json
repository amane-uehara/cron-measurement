{
  "constant": {
    "warning" : "This `constant` block will be over written. Do not edit this file.",
    "google_calendar_savefile"    : "/path/to/raw/filename",
    "google_calendar_token"       : "/path/to/token.json",
    "google_calendar_credentials" : "/path/to/credentials.json",
    "google_calendar_start"       : "${yyyymmdd-1}000000",
    "google_calendar_end"         : "${yyyymmdd+1}000000"
  },

  "job_google_calendar_raw_json": {
    "item"                 : "google_calendar",
    "sensor"               : "google_calendar",
    "program"              : "fetch_raw_json",
    "savefile"             : "${google_calendar_savefile}",
    "token_json_path"      : "${google_calendar_token}",
    "credentials_json_path": "${google_calendar_credentials}",
    "start_yyyymmddhhmmss" : "${google_calendar_start}",
    "end_yyyymmddhhmmss"   : "${google_calendar_end}"
  }
}
