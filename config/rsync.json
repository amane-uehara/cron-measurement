{
  "constant": {
    "warning" : "This `constant` block will be over written. Do not edit this file.",

    "hostname"      : "example_host",
    "rsync_src"     : "/path/to/local/src/dir",
    "rsync_dst_arr" : "server1:/path/to/sv1/dst/dir server2:/path/to/sv2/dst/dir"
  },

  "job_rsync": {
    "sensor"       : "shell",
    "program"      : "shell",
    "command_list" : [
      "for dst in ${rsync_dst_arr} ;do rsync -av ${rsync_src}/ ${dst}/ ;done"
    ]
  }
}
